<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="IntelliHouse2000App.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:IntelliHouse2000App"
    xmlns:view="clr-namespace:IntelliHouse2000App.Views"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem></FlyoutItem>
    <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate view:MainPage}"/>

    <ShellContent 
            Title="Alarm"
            ContentTemplate="{DataTemplate view:Alarm}" />

    <ShellContent 
            Title="Climate"
            ContentTemplate="{DataTemplate view:Climate}" />

</Shell>

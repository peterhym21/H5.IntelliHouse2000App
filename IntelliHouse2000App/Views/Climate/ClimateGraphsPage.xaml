<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IntelliHouse2000App.Views.ClimateGraphsPage"
             xmlns:model="clr-namespace:IntelliHouse2000App.Models"
             xmlns:climate="clr-namespace:IntelliHouse2000App.ViewModels.Climate"
             x:DataType="climate:ClimateGraphsPageViewModel">

    <ContentView>
        <VerticalStackLayout
            Spacing="25"
            Padding="30,0"
            VerticalOptions="Center">

            <Label Text="{Binding KitchenValues.Count }" />
            <Label Text="{Binding BedroomValues.Count}" />
            <Label Text="{Binding LivingroomValues.Count}" />
            

        </VerticalStackLayout>
    </ContentView>

</ContentPage>

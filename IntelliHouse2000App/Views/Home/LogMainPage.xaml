<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="IntelliHouse2000App.Views.LogMainPage"
             xmlns:views="clr-namespace:IntelliHouse2000App.Views"
             Title="LogPage">
    <TabBar>
        <Tab Title="Logs">
            <ShellContent 
                Title="Critical logs"
                ContentTemplate="{DataTemplate views:CriticalLogPage}" />
            <ShellContent 
                Title="Info logs"
                ContentTemplate="{DataTemplate views:InfoLogPage}" />
            <ShellContent 
                Title="System logs"
                ContentTemplate="{DataTemplate views:SystemLogPage}" />
        </Tab>
    </TabBar>
</Shell>